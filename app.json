{
  "expo": {
    "name": "SerenGuard",
    "slug": "SerenGuard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "za.co.openwindow.serenguard"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/adaptive-icon.png",
        "backgroundColor": "#151414"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "za.co.openwindow.serenguard"
    },
    "web": {
      "favicon": "src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "SerenGuard accesses your photos to let you share them with your community."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysInUsePermission": "Allow SerenGuard to use your location?"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "54226094-b65a-46bc-9f33-d6f876a336dc"
      }
    },
    "scheme": "serenguard"
  }
}
