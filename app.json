{
  "expo": {
    "name": "SerenGuard",
    "slug": "SerenGuard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/adaptive-icon.png",
        "backgroundColor": "#151414"
      }
    },
    "web": {
      "favicon": "src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "SerenGuard accesses your photos to let you share them with your community."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysInUsePermission": "Allow SerenGuard to use your location?"
        }
      ]
    ]
  }
}
